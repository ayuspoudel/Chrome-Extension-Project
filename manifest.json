{
  "name": "Chrome Extension",
  "description": "Go from light mode to dark mode and vice versa",
  "version": "1.0.0",
  "manifest_version": 2,
  "background": {
    "Scripts": ["popup.js"],
    "persistent": true
  },
  "browser_action": {
    "default_file": "Appearence",
    "default_popup": "popup.html"
  },
  "permissions": ["https://*/*", "http://*/*", "tabs"],
  "author": "Ayush Poudel"
}
